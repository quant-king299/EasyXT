    "eastmoney": {
      "enabled": true,
      "timeout": 25,
      "retry_count": 3,
      "retry_delay": 1.0,
      "rate_limit": 150,
      "headers": {
        "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36",
        "Referer": "http://quote.eastmoney.com/"
      },
      "base_urls": {
