        "kline": "http://push2his.eastmoney.com/api/qt/stock/kline/get"
      }
    }
  },
  "cache": {
    "enabled": true,
    "backend": "memory",
    "ttl": 300,
    "max_size": 10000,
    "redis_host": "localhost",
    "redis_port": 6379,
    "redis_db": 0,
    "cleanup_interval": 3600
  },
  "websocket": {
    "host": "localhost",
    "port": 8765,
    "max_connections": 100,
    "heartbeat_interval": 30,
    "message_queue_size": 1000,
    "compression": true,
    "ping_timeout": 20,
    "ping_interval": 25
  },
  "monitor": {
    "enabled": true,
    "metrics_interval": 60,
    "alert_thresholds": {
      "cpu_usage": 80.0,
      "memory_usage": 85.0,
      "error_rate": 5.0,
      "response_time": 3.0,
      "connection_count": 90
    },
    "notification_channels": {
      "console": {
        "enabled": true,
        "level": "WARNING"
      },
      "file": {
        "enabled": true,
        "level": "ERROR",
        "file_path": "logs/alerts.log"
      }
    },
    "data_quality_checks": {
      "enabled": true,
      "min_data_points": 10,
      "max_deviation": 0.1
    }
  },
  "scheduler": {
    "enabled": true,
    "max_workers": 10,
    "task_timeout": 300,
    "retry_policy": {
      "max_retries": 3,
      "backoff_factor": 2.0,
      "max_delay": 300
    },
    "task_priorities": {
      "realtime_data": "HIGH",
      "historical_data": "MEDIUM",
      "cleanup": "LOW"
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file_path": "logs/realtime_data.log",
    "max_file_size": "10MB",
    "backup_count": 5,
    "console_output": true,
    "structured_logging": false
  },
  "security": {
    "api_key_required": false,
    "rate_limiting": {
      "enabled": true,
      "requests_per_minute": 1000,
      "burst_size": 100
    },
    "cors": {
      "enabled": true,
      "allowed_origins": [
        "*"
      ],
      "allowed_methods": [
        "GET",
        "POST",
        "OPTIONS"
      ]
    }
  },
  "performance": {
    "connection_pool_size": 20,
    "request_timeout": 30,
    "max_concurrent_requests": 50,
    "batch_size": 100,
    "prefetch_enabled": true,
    "compression_enabled": true
  },
  "global": {
    "test": "global_value",
    "demo": "global_value"
  }
}